    "lint": "exit 0",
    "test": "exit 0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@orama/orama": "workspace:*",
    "@types/node": "^20.9.0",
    "c8": "^7.12.0",
    "sinon": "^17.0.1",
    "tap": "^18.6.1",
    "tap-mocha-reporter": "^5.0.3",
    "tape": "^5.6.1",
    "ts-standard": "^12.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0"
  },
  "tap": {
    "extends": "src/tests/config/tap.yml"
  },
  "dependencies": {
    "@oramacloud/client": "^1.3.11"
  }
